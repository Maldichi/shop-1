{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "version": "1.0.0-beta.32.3",
    "name": "calculator-angular"
  },
  "apps": [
    {
      "root": ".",
      "outDir": "dist",
      "assets": [
        "resources"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "debug/test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "styles": [
        "resources/styles.css"
      ],
      "scripts": [],
      "environmentSource": "debug/environments/environment.ts",
      "environments": {
        "dev": "debug/environments/environment.ts",
        "prod": "debug/environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./debug/protractor.conf.js"
    }
  },
  "lint": [
    {
      "files": "./**/*.ts",
      "project": "./tsconfig.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./debug/karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
